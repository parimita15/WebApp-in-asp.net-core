@model Category
<form method="post">
    <div class="border p-3 mt-4">
        <div class="row pb-3">
            <h2 class="text-primary" >
                Category
            </h2>
        </div>

        <div typeof="hidden" asp-for="Id"></div>

        <div asp-validation-summary="All"></div>
        <div class="row mb-3">
            <label asp-for="Name"></label>
            <input disabled asp-for="Name"  class="form-control" />
            <span asp-validation-for="Name"  class="text-danger"></span>
        </div>

        <div class="row mb-3">
            <label  asp-for="DisplayOrder"></label>
            <input disabled asp-for="DisplayOrder" class="form-control" />
            <span asp-validation-for="DisplayOrder" class="text-danger"></span>
        </div>
        <button class="btn btn-danger" type="submit">Delete</button>
    </div>
    <a asp-action="Index" asp-controller="Category">Back to List</a>
</form>



@section scripts{
    @{

        <partia name="_validationScriptspartial" />
    }
}